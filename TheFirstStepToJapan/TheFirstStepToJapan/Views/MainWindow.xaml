<Window x:Class="TheFirstStepToJapan.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TheFirstStepToJapan"
        mc:Ignorable="d"
        x:Name="AppWindow"
        Title="Первый шаг к Японии" Height="600" Width="800" Background="#FF157EE8" Icon="ico.ico">

    <Grid>

        <Grid.Background>
            <LinearGradientBrush EndPoint="1,1" MappingMode="RelativeToBoundingBox" StartPoint="0,0">
                <GradientStop Color="#FF0D5DAC" Offset="1"/>
                <GradientStop Color="#FF44A2CD" Offset="0.342"/>
                <GradientStop Color="#FF0D5DAC"/>
                <GradientStop Color="#FF247ABA" Offset="0.738"/>
            </LinearGradientBrush>
        </Grid.Background>

        <Frame
                x:Name="frm"
                NavigationUIVisibility="Hidden"
                Margin="0,0,0,0"
                />

    </Grid>



</Window>
